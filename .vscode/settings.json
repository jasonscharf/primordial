{
    "jupyter.jupyterServerType": "local",
    "python.analysis.logLevel": "Trace",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__/**": true,
        "**/env/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/env/**": true,
        "**/*.egg-info/**": true,
        "**/temp": true,
    },
    "python.pythonPath": "/usr/bin/python3"
}