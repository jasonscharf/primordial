{
  "compilerOptions": {
    "target": "esnext",
    "module": "umd",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "importHelpers": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false
  }
}
