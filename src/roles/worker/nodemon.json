{
    "verbose": false,
    "inspect": true,
    "exec": "node --nolazy --inspect=0.0.0.0:9229 /app/worker/worker.js",
    "watch": [
        "/app/**/*.js",
        "**/nodemon.json"
    ],
    "ext": "js,json",
    "delay": 500,
    "ignore": [
        "*.d.ts",
        "*.map"
    ],
    "execMap": {},
    "events": {
        "restart": "echo \"--- RESTART ---\""
    }
}